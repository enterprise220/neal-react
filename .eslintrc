{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "analytics": true,
    "StripeCheckout": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "arrow-spacing": 2,
    "block-spacing": [2, "always"],
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [2, 2],
    "jsx-quotes": [2, "prefer-double"],
    "max-len": [1, 120],
    "no-const-assign": 2,
    "no-dupe-keys": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": 0,
    "no-var": 2,
    "object-curly-spacing": [2, "always"],
    "prefer-arrow-callback": 2,
    "prefer-template": 2,
    "quotes": [2, "double", "avoid-escape"],
    "semi": [2, "always"],
    "space-before-blocks": 2,
    "space-infix-ops": 2,
  }
}